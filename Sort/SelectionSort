#include <stdio.h>
#include <windows.h>
#pragma warning (disable : 4996)


void sort_bubble(int* arr, int size)
{
	int temp=0;
	for (int i = 0; i < size; i++)
	{
		for (int j = 0; j < size - 1 - i; j++)
		{
			if (arr[j] > arr[j + 1])
			{
				temp = arr[j];
				arr[j] = arr[j + 1];
				arr[j + 1] = temp;
			}
		}
	}
	for (int i = 0; i < size; i++)
	{
		printf("%d", arr[i]);
		
	}

}
